class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        # Create a Hashmap to store the value and its index!
        hash = {} 
        # Iterate through the list
        for i, n in enumerate(nums):
            diff = target - n
            if diff in hash:
                return [hash[diff], i]
            hash[n] = i
        return
