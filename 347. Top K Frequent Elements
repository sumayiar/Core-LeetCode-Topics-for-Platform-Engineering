class Solution:
    def topKFrequent(self, nums: List[int], k: int) -> List[int]:
        #dictionary will keep count
        count = {}
        #will keep track of how frequent elements appear in the array
        freq =[[] for i in range(len(nums) + 1)] 

        #go through the array
        for n in nums:
            #in the dictionary, 
            count[n] = 1 + count.get(n,0)
        for n, c in count.items():
            freq[c].append(n)

        res = []
        for i in range(len(freq) - 1, 0, -1):
            for n in freq[i]:
                res.append(n)
                if len(res) == k:
                    return res

#worked on a medium level problem
#exercised a common Leetcode pattern
